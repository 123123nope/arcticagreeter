msgid "Suspend"
msgstr "معلّق کنید"

#: ../src/shutdown-dialog.vala:172
msgid "Hibernate"
msgstr "به خواب زمستانی ببرید"

#: ../src/shutdown-dialog.vala:190
msgid "Restart"
msgstr "شروع مجدد"

