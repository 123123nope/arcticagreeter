msgid "X2Go Session:"
msgstr "X2Go Сесия:"

#: ../src/user-list.vala:729
msgid "Domain:"
msgstr "Домейн:"

#: ../src/user-list.vala:790
msgid "Email:"
msgstr "Адрес на е-поща:"

